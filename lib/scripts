  Artist.all.each do |artist|
   @slug = artist.slug
   url = "https://graph.facebook.com/?id=http://radioperu.pe/es/artists/#{@slug}&scrape=true"
   system("curl --insecure '#{url}'")
   sleep 5
  end

  Artist.all.each do |artist|
   @slug = artist.slug
   artist.albums.each do |album|
    albumslug = album.slug
      albumurl = "https://graph.facebook.com/?id=http://radioperu.pe/es/artists/#{@slug}/albums/#{albumslug}&scrape=true"
      system("curl --insecure '#{albumurl}'")
      sleep 5
    end
  end

  Artist.all.each do |artist|
   @slug = artist.slug
   artist.albums.each do |album|
    @albumslug = album.slug
      album.songs.each do |song|
        songslug = album.slug
        songurl = "https://graph.facebook.com/?id=http://radioperu.pe/es/artists/#{@slug}/albums/#{@albumslug}/songs/#{songslug}&scrape=true"
        system("curl --insecure '#{albumurl}'")
        sleep 5
      end
    end
  end

