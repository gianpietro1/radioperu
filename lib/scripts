songpath = song.filename.file.file

TagLib::FileRef.open(songpath) do |fileref|
  unless fileref == nil
    properties = fileref.audio_properties
    if properties.bitrate != 128
      system("ffmpeg -i '#{songpath}' -y -b:a 128k -vsync 2 '#{songpath[0..-5] + '-temp' + '.mp3'}'")
      system("mv '#{songpath[0..-5] + '-temp' + '.mp3'}' '#{songpath}'")    
    end
  end
end

