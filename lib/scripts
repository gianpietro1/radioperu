      TagLib::FileRef.open(songpath) do |fileref|
        unless fileref.null?
          properties = fileref.audio_properties
            system("avconv -i '#{songpath}' -c:a libmp3lame -y -vn -b:a 128k -vsync 2 '#{songpath[0..-5] + '-temp' + '.mp3'}'")
            system("mv '#{songpath[0..-5] + '-temp' + '.mp3'}' '#{songpath}'")    
        end
      end



