  Artist.all.each do |artist|
   @slug = artist.slug
   url = "https://graph.facebook.com/?id=http://radioperu.pe/es/artists/#{@slug}&scrape=true&access_token=440598459358292|GVj1kcc9QwokzIRBDoNAkznYhQM"
   system("curl -X POST --insecure '#{url}'")
   sleep 2
  end

  Artist.all.reverse_each do |artist|
   @slug = artist.slug
   artist.albums.each do |album|
    albumslug = album.slug
      albumurl = "https://graph.facebook.com/?id=http://radioperu.pe/es/artists/#{@slug}/albums/#{albumslug}&scrape=true&access_token=440598459358292|GVj1kcc9QwokzIRBDoNAkznYhQM"
      system("curl -X POST --insecure '#{albumurl}'")
      sleep 2
    end
  end

  Artist.all.each do |artist|
   @slug = artist.slug
   artist.albums.each do |album|
    @albumslug = album.slug
      album.songs.each do |song|
        songslug = album.slug
        songurl = "https://graph.facebook.com/?id=http://radioperu.pe/es/artists/#{@slug}/albums/#{@albumslug}/songs/#{songslug}&scrape=true&access_token=440598459358292|GVj1kcc9QwokzIRBDoNAkznYhQM"
        system("curl -X POST --insecure '#{songurl}'")
        sleep 2
      end
    end
  end

